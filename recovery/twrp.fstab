#mount point	fstype	device			[device2]			fstype2
/boot		emmc	/dev/block/bootdevice/by-name/boot
/recovery	emmc	/dev/block/bootdevice/by-name/recovery
/system		ext4	/dev/block/bootdevice/by-name/system
/cache		ext4	/dev/block/bootdevice/by-name/cache
/data		ext4	/dev/block/bootdevice/by-name/userdata			length=-16384
/preload	ext4	/dev/block/bootdevice/by-name/hidden			flags=wipeingui;backup=1
/persistent	emmc	/dev/block/platform/7824900.sdhci/by-name/persistent
/mdm		emmc	/dev/block/bootdevice/by-name/modem
/modem		emmc	/dev/block/bootdevice/by-name/apnhlos
/efs		ext4	/dev/block/bootdevice/by-name/efs

/sdcard		vfat	/dev/block/mmcblk1p1	/dev/block/mmcblk1		flags=storage;wipeingui;removable;display="Micro SDcard"
/usb-otg	vfat	/dev/block/sda1		/dev/block/sda			flags=storage;wipeingui;removable;display="USB-OTG"
